<body>
  <div style="margin-top:5%;"></div>

  <div class="container" style="width:60%;">
    <table id="example">
        <thead>
            <tr>
                <th scope="col">USER ID</th>
                <th scope="col">ID</th>
                <th scope="col">TITLE</th>
                <th scope="col">COMPLETED</th>
            </tr>
