<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/scroller/2.0.7/css/scroller.dataTables.min.css">
<script type="text/javascript" charset="utf8" src="/DataTables/datatables.js"></script> 
<script type="text/javascript" language="javascript" src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script type="text/javascript" language="javascript" src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" language="javascript" src="https://cdn.datatables.net/buttons/1.2.3/js/dataTables.buttons.min.js">
</script>
<script type="text/javascript" language="javascript" src="https://cdn.datatables.net/scroller/2.0.7/js/dataTables.scroller.js">
</script>

<script type="text/javascript" class="init">

$(document).ready(function() { 
  

  // Create a new DataTable object
  table = $('#example').DataTable({


       "lengthMenu": [ [15, 50, 100, -1], [15, 50, 100, "All"] ],
       "pagingType": "simple",
       scrollY: 400,
       select: true,
    deferRender:    true,
    scroller:       true,
       scrollCollapse: true,
       order: [[ 1, 'asc' ]],
       ajax: {
         url: '/roomData',
       },
       columns: [
           { data: '_id',
                visible: false, },
           { data: 'roomID' },
           { data: 'price' },
           { data: 'promotionalCode' },
           { data: 'roomCapacity' },
           { data: 'timeslot',
                visible: false },
           { data: 'createdBy',
                visible: false },
           { data: 'bookedBy',
                visible: false },
           { data: 'launchStatus' },
           { data: 'launchstartdate' },
           { data: 'launchenddate' }
         ]
       })


       table = $('#lala').DataTable({


            "lengthMenu": [ [15, 50, 100, -1], [15, 50, 100, "All"] ],
            "pagingType": "simple",
            scrollY: 400,
            select: false,

            deferRender:    true,
    scroller:       true,
            scrollCollapse: true,
            order: [[ 1, 'asc' ]],
            ajax: {
              url: '/roomData',
            },
            columns: [
                { data: '_id',
                     visible: false, },
                { data: 'roomID' },
                { data: 'price' },
                { data: 'promotionalCode' },
                { data: 'roomCapacity' },
                { data: 'timeslot' },
                { data: 'createdBy' },
                { data: 'bookedBy' },
                { data: 'launchStatus' },
				
           { data: 'launchstartdate' },
           { data: 'launchenddate' }
              ]
            }) 
       

       $('#example tbody').on('click', 'tr', function () {
        var data = table.row(this).data();

        if ($(this).hasClass('selected')) {
            $(this).removeClass('selected');
        } else {
            table.$('tr.selected').removeClass('selected');
            $(this).addClass('selected');
            obtainroomID= data.roomID
            localStorage.setItem('room-ID',obtainroomID)
        }

    })
 
} );;

 




</script>
